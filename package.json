{
  "name": "jet",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "commit": "git add . && npx --no -- commit",
    "commitlint": "npx --no -- commitlint --edit $1",
    "deploy": "npx --no -- ng deploy --base-href=/jet/",
    "i18n:extract": "npx --no -- transloco-keys-manager extract",
    "i18n:find": "npx --no -- transloco-keys-manager find",
    "lint": "npx --no -- ng lint && npx --no -- lint-staged",
    "prepare": "npx --no -- husky",
    "install-dependencies": "npm install @angular/animations @angular/cdk@17 @angular/common @angular/compiler @angular/core @angular/forms @angular/material@17 @angular/platform-browser @angular/platform-browser-dynamic @angular/router @angular/service-worker @jsverse/transloco rxjs tslib zone.js",
    "uninstall-dependencies": "npm uninstall @angular/animations @angular/cdk @angular/common @angular/compiler @angular/core @angular/forms @angular/material @angular/platform-browser @angular/platform-browser-dynamic @angular/router @angular/service-worker @jsverse/transloco rxjs tslib zone.js",
    "install-devDependencies": "npm install --save-dev @angular-devkit/build-angular @angular/cli @angular/compiler-cli @commitlint/cli @commitlint/config-angular @commitlint/prompt-cli @jsverse/transloco-keys-manager @types/jasmine angular-cli-ghpages angular-eslint eslint husky jasmine-core karma karma-chrome-launcher karma-coverage karma-jasmine karma-jasmine-html-reporter lint-staged ngx-build-plus prettier typescript typescript-eslint",
    "uninstall-devDependencies": "npm uninstall @angular-devkit/build-angular @angular/cli @angular/compiler-cli @commitlint/cli @commitlint/config-angular @commitlint/prompt-cli @jsverse/transloco-keys-manager @types/jasmine angular-cli-ghpages angular-eslint eslint husky jasmine-core karma karma-chrome-launcher karma-coverage karma-jasmine karma-jasmine-html-reporter lint-staged ngx-build-plus prettier typescript typescript-eslint",
    "reinstall-dependencies": "npm run uninstall-devDependencies && npm run uninstall-dependencies && npm run install-dependencies && npm run install-devDependencies && npm run build"
  },
  "private": true,
  "lint-staged": {
    "*": "npx --no -- prettier --ignore-unknown --write"
  },
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "@angular/service-worker": "^18.0.2",
    "@jsverse/transloco": "^7.4.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.0.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@jsverse/transloco-keys-manager": "^4.2.0",
    "@types/jasmine": "^5.1.4",
    "angular-cli-ghpages": "^2.0.0",
    "angular-eslint": "^18.0.1",
    "eslint": "^9.4.0",
    "husky": "^9.0.11",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.5",
    "ngx-build-plus": "^18.0.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.28"
  }
}
